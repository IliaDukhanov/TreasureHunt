[32m+[m[32m        private void columns_ValueChanged(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            gameField.ColumnCount = (int)columns.Value;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void rows_ValueChanged(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            gameField.RowCount = (int)rows.Value;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void hydro_ValueChanged(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            player.HydroCount = (int)hydro.Value;[m
[32m+[m[32m        }[m
[32m+[m[32m        private void treasure_ValueChanged(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            player.TreasureCount = (int)treasure.Value;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void area_ValueChanged(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            areaOfHydro = (int)area.Value;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void button1_Click(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            startGame(true);[m
[32m+[m[32m        }[m
[32m+[m[32m        private void gameField_CellClick(object sender, DataGridViewCellEventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            if ((player.HydroCount > 0) && (player.TreasureCount > 0) && (gameField[gameField.CurrentCell.ColumnIndex, gameField.CurrentCell.RowIndex].Value == null))[m
[32m+[m[32m            {[m
[32m+[m[32m                hydros.Add(new Hydro(new Point(gameField.CurrentCell.RowIndex, gameField.CurrentCell.ColumnIndex), areaOfHydro));[m
[32m+[m[32m                foundedTreasure = hydros.Last().findTreasure(treasures);[m
[32m+[m[32m                if ((foundedTreasure.X < 500) && (foundedTreasure.Y < 500))[m
[32m+[m[32m                {[m
[32m+[m[32m                    if (Math.Abs(foundedTreasure.X - hydros.Last().Position.X) > Math.Abs(foundedTreasure.Y - hydros.Last().Position.Y))[m
[32m+[m[32m                        gameField[hydros.Last().Position.Y, hydros.Last().Position.X].Value = Math.Abs(foundedTreasure.X - hydros.Last().Position.X);[m
[32m+[m[32m                    else if ((foundedTreasure == hydros.Last().Position) && (player.TreasureCount > 0))[m
[32m+[m[32m                    {[m
[32m+[m[32m                        gameField[foundedTreasure.Y, foundedTreasure.X].Value = 0;[m
[32m+[m[32m                        treasures.Remove(treasures.Find(item => item.Position == foundedTreasure));[m
[32m+[m[32m                        player.TreasureCount -= 1;[m
[32m+[m[32m                        for (int i = 0; i < hydros.Count; i++)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            foundedTreasure = hydros[i].findTreasure(treasures);[m
[32m+[m[32m                            if ((foundedTreasure.X < 500) && (foundedTreasure.Y < 500) && !(gameField[hydros[i].Position.Y, hydros[i].Position.X].Value.Equals(0)))[m
[32m+[m[32m                            {[m
[32m+[m[32m                                if (Math.Abs(foundedTreasure.X - hydros[i].Position.X) > Math.Abs(foundedTreasure.Y - hydros[i].Position.Y))[m
[32m+[m[32m                                    gameField[hydros[i].Position.Y, hydros[i].Position.X].Value = Math.Abs(foundedTreasure.X - hydros[i].Position.X);[m
[32m+[m[32m                                else[m
[32m+[m[32m                                    gameField[hydros[i].Position.Y, hydros[i].Position.X].Value = Math.Abs(foundedTreasure.Y - hydros[i].Position.Y);[m
[32m+[m[32m                            }[m
[32m+[m[32m                            else if (!gameField[hydros[i].Position.Y, hydros[i].Position.X].Value.Equals(0))[m
[32m+[m[32m                                gameField[hydros[i].Position.Y, hydros[i].Position.X].Value = 'X';[m
[32m+[m[32m                        }[m
[32m+[m[32m                        if (player.TreasureCount <= 0)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            label9.Text = player.HydroCount.ToString();[m
[32m+[m[32m                            label11.Text = player.TreasureCount.ToString();[m
[32m+[m[32m                            player.WinCount += 1;[m
[32m+[m[32m                            MessageBox.Show("–ü–æ–∑–¥—Ä–∞–≤–ª—è—é!!!\n" + "–°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞:\n" + player.WinCount + "-–ø–æ–±–µ–¥\n" + player.LooseCount + "-–ø–æ—Ä–∞–∂–µ–Ω–∏–π", "–ü–æ–±–µ–¥–∞!");[m
[32m+[m[32m                            startGame(false);[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                    else[m
[32m+[m[32m                        gameField[hydros.Last().Position.Y, hydros.Last().Position.X].Value = Math.Abs(foundedTreasure.Y - hydros.Last().Position.Y);[m
[32m+[m[32m                }[m
[32m+[m[32m                else[m
[32m+[m[32m                    gameField[hydros.Last().Position.Y, hydros.Last().Position.X].Value = 'X';[m
[32m+[m[32m                player.HydroCount -= 1;[m
[32m+[m[32m                if ((player.TreasureCount >= 0) && (player.HydroCount <= 0))[m
[32m+[m[32m                {[m
[32m+[m[32m                    label9.Text = player.HydroCount.ToString();[m
[32m+[m[32m                    label11.Text = player.TreasureCount.ToString();[m
[32m+[m[32m                    player.LooseCount += 1;[m
[32m+[m[32m                    MessageBox.Show("–°–æ–∂–∞–ª–µ—é...\n" + "–°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞:\n" + player.WinCount + "-–ø–æ–±–µ–¥\n" + player.LooseCount + "-–ø–æ—Ä–∞–∂–µ–Ω–∏–π", "–ü–æ—Ä–∞–∂–µ–Ω–∏–µ");[m
[32m+[m[32m                    for (int i = 0; i < treasures.Count; i++)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        gameField[treasures[i].Position.Y, treasures[i].Position.X].Value = 'T';[m
[32m+[m[32m                    }[m
[32m+[m[32m                    startGame(false);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            label9.Text = player.HydroCount.ToString();[m
[32m+[m[32m            label11.Text = player.TreasureCount.ToString();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        void startGame(bool start)[m
[32m+[m[32m        {[m
[32m+[m[32m            gameField.Enabled = start;[m
[32m+[m[32m            columns.Enabled = !start;[m
[32m+[m[32m            rows.Enabled = !start;[m
[32m+[m[32m            treasure.Enabled = !start;[m
[32m+[m[32m            hydro.Enabled = !start;[m
[32m+[m[32m            area.Enabled = !start;[m
[32m+[m[32m            if (start)[m
[32m+[m[32m            {[m
[32m+[m[32m                hydros.Clear();[m
[32m+[m[32m                player.HydroCount = (int)hydro.Value;[m
[32m+[m[32m                player.TreasureCount = (int)treasure.Value;[m
[32m+[m[32m                for (int i = 0; i < gameField.ColumnCount; i++)[m
[32m+[m[32m                {[m
[32m+[m[32m                    for (int j = 0; j < gameField.RowCount; j++)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        gameField[i, j].Value = null;[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            label9.Text = hydro.Value.ToString();[m
[32m+[m[32m            label11.Text = treasure.Value.ToString();[m
[32m+[m[32m            while (treasures.Count < player.TreasureCount)[m
[32m+[m[32m            {[m
[32m+[m[32m                Point position = new Point(new Random(DateTime.Now.Millisecond).Next(0, gameField.RowCount), new Random(DateTime.Now.Millisecond).Next(0, gameField.ColumnCount));[m
[32m+[m[32m                if (treasures.FindAll(item => item.Position == position).Count == 0)[m
[32m+[m[32m                    treasures.Add(new Treasure(position));[m
[32m+[m[32m            }[m
[32m+[m[32m            button1.Enabled = !start;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
