[32m+[m[32mÔªøusing System;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing System.Drawing;[m
[32m+[m
[32m+[m[32mnamespace WindowsFormsApp1[m
[32m+[m[32m{[m
[32m+[m[32m    class Hydro:Treasure[m
[32m+[m[32m    {[m
[32m+[m[32m        public Hydro(Point position, int area):base(position)[m
[32m+[m[32m        {[m
[32m+[m[32m            this.area = area;[m
[32m+[m[32m        }[m
[32m+[m[32m        private int area;[m
[32m+[m
[32m+[m[32m        public Point findTreasure(List<Treasure> treasures)[m
[32m+[m[32m        {[m
[32m+[m[32m            Point foundedTreasure = new Point(999,999);[m
[32m+[m[32m            for (int i = 0; i < treasures.Count; i++)[m
[32m+[m[32m            {[m
[32m+[m[32m                if ((Math.Abs(treasures[i].Position.X - Position.X) <= area) && (Math.Abs(treasures[i].Position.Y - Position.Y) <= area))[m
[32m+[m[32m                {[m
[32m+[m[32m                    if((Math.Abs(treasures[i].Position.X - Position.X) <= Math.Abs(foundedTreasure.X - Position.X)) && (Math.Abs(treasures[i].Position.Y - Position.Y) <= Math.Abs(foundedTreasure.Y - Position.Y)))[m
[32m+[m[32m                        foundedTreasure = treasures[i].Position;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            return foundedTreasure;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
