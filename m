[32m+[m[32m        }[m
[32m+[m[32m        public int HydroCount[m
[32m+[m[32m        {[m
[32m+[m[32m            get[m
[32m+[m[32m            {[m
[32m+[m[32m                return hydroCount;[m
[32m+[m[32m            }[m
[32m+[m[32m            set[m
